citHeader("To cite spatzie in publications use:")
citEntry(
  entry = "article",
  title = "spatzie: An R package for identifying significant transcription factor motif co-enrichment from enhancer-promoter interactions",
  author = personList(as.person("Jennifer Hammelman"),
                      as.person("Konstanin Krismer"),
                      as.person("David K. Gifford")),
  journal = "bioRxiv",
  pages = "2021.05.25.445606",
  year = 2021,
  month = 5,
  doi = "10.1101/2021.05.25.445606",
  URL = "https://doi.org/10.1101/2021.05.25.445606",
	publisher = "Cold Spring Harbor Laboratory",
  textVersion =
    paste("spatzie: An R package for identifying significant transcription factor motif co-enrichment from enhancer-promoter interactions",
          "Jennifer Hammelman, Konstantin Krismer, David K. Gifford",
          "bioRxiv 2021.05.25.445606; DOI: https://doi.org/10.1101/2021.05.25.445606",
          sep = "\n")
)
citFooter("This free open-source software implements academic
research by the authors and co-workers. If you use it,
please support the project by citing the appropriate
article.")
